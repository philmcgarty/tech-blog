
<ol class="blog-list">
  {{#each blogs as |blog|}}
  <li>
    <article class="blog">
      <div class="title">
        
        <span>{{article}}</span>
      </div>
      <div class="meta">
        by {{blog.user.username}} on
        {{blog.created_at}}
        |
        <a href="/blog/{{blog.id}}">{{blog.comments.length}} comment(s)</a>
      </div>
    </article>
  </li>
  {{/each}}
</ol>